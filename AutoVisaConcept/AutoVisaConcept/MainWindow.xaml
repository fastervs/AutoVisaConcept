<Window x:Class="AutoVisaConcept.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AutoVisaConcept"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <DataGrid x:Name="dataGrid" HorizontalAlignment="Left" Height="204" Margin="32,39,0,0" VerticalAlignment="Top" Width="348"/>
        <ComboBox x:Name="comboBox" HorizontalAlignment="Left" Margin="600,74,0,0" VerticalAlignment="Top" Width="140"/>
        <Button x:Name="button" Content="Загрузить сканы" Command="{Binding Download}" HorizontalAlignment="Left" Margin="445,65,0,0" VerticalAlignment="Top" Width="94" Height="31"/>
        <Button x:Name="button1" Content="Внести данные" HorizontalAlignment="Left" Margin="445,164,0,0" VerticalAlignment="Top" Width="94" Height="27"/>
        <Button x:Name="button2" Content="Удалить" HorizontalAlignment="Left" Margin="649,186,0,0" VerticalAlignment="Top" Width="75"/>
    </Grid>
</Window>
